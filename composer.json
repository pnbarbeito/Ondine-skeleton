{
  "name": "pnbarbeito/ondine-skeleton",
  "description": "Skeleton project to start an Ondine-based application",
  "type": "project",
  "license": "MIT",
  "require": {
    "pnbarbeito/ondine": "v1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "guzzlehttp/guzzle": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
